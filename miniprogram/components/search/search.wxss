.container{
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  position: relative;
}
.bar{
  flex: 1;
  height: 68rpx;
  font-size: 28rpx;
  border-radius: 50rpx;
  background: #f5f5f5;
  padding-left: 60rpx;
}
.search{
  background: #d43c33;
  color: #fff;
  margin-left: 20rpx;
  font-size: 24rpx;
}
.in-bar{
  color:#999;
}
.icon-search{
  position: absolute;
  top:50%;
  transform: translate(16rpx, -50%);
  color:#9f9f9f;
}

